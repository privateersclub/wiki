    "@nolebase/vitepress-plugin-page-properties": "^2.5.0",
    "@types/node": "^22.6.1",
    "@types/nprogress": "^0.2.3",
    "prettier": "^3.3.3",
    "sass": "^1.79.3",
    "vitepress": "npm:@taskylizard/vitepress@1.1.1"
  },
  "dependencies": {
    "@lunariajs/core": "^0.1.1",
    "@resvg/resvg-js": "^2.6.2",
    "nprogress": "^0.2.0",
    "unocss": "^0.62.4",
    "vue": "^3.5.8",
    "x-satori": "^0.2.0"
  }
