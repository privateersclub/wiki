{
  "name": "wiki",
  "version": "1.0.0",
  "type": "module",
  "main": "index.js",
  "scripts": {
    "format": "prettier --cache --write docs/",
    "build": "pnpm run docs:build && pnpm run i18n:build",
    "docs:dev": "vitepress dev docs",
    "docs:build": "vitepress build docs",
    "docs:serve": "vitepress serve docs",
    "docs:preview": "vitepress preview docs",
    "i18n:build": "lunaria build",
    "i18n:preview": "lunaria preview",
    "og:dev": "x-satori -t ./docs/.vitepress/hooks/Template.vue -c ./docs/.vitepress/hooks/satoriConfig.ts --dev"
  },
  "license": "Unlicense",
  "devDependencies": {
    "@coloradix/unocss": "^2.3.2",
    "@iconify-json/logos": "^1.2.10",
    "@lunariajs/core": "^0.1.1",
    "@nolebase/vitepress-plugin-enhanced-readabilities": "^2.18.2",
    "@nolebase/vitepress-plugin-git-changelog": "^2.18.2",
    "@nolebase/vitepress-plugin-page-properties": "^2.18.2",
    "@radix-ui/colors": "^3.0.0",
    "@resvg/resvg-js": "^2.6.2",
    "@types/node": "^25.3.2",
    "@types/nprogress": "^0.2.3",
    "nprogress": "^0.2.0",
    "prettier": "^3.8.1",
    "sass": "^1.97.3",
    "unocss": "^66.6.2",
    "vitepress": "2.0.0-alpha.16",
    "x-satori": "^0.4.0"
  },
  "dependencies": {
    "@iconify/vue": "^5.0.0",
    "markdown-it-anchor": "^9.2.0",
    "oxc-minify": "^0.115.0",
    "vue": "^3.5.29"
  },
  "pnpm": {
    "overrides": {
      "vite": "8.0.0-beta.0"
    }
  },
  "packageManager": "pnpm@9.15.6"
